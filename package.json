{
  "name": "portfolio-react",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "homepage": "https://sagargupta.live/portfolio-react/",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "type-check": "tsc --noEmit",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build dist node_modules/.vite",
    "prepare": "husky"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "framer-motion": "^12.23.24",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "react-simple-typewriter": "^5.0.1",
    "swiper": "^12.0.3",
    "typewriter-effect": "^2.22.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=24.11.0",
    "pnpm": ">=10.0.0"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "description": "A dynamic and interactive React portfolio showcasing professional experience as a DevOps & Cloud Engineer. Features modern JSON-based data architecture, responsive design, and seamless deployment with CI/CD pipelines.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sagargupta16/portfolio-react.git"
  },
  "author": "Sagar Gupta <sg85207@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sagargupta16/portfolio-react/issues"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
